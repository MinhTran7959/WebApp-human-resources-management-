@model IEnumerable<QLNS_LTW.Models.ToanBoDanhSachKyLuatResult>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/layout_kyluat.cshtml";
}


<table class="table">
    <tr>
        
        <th>
            @Html.DisplayName("THỜI GIAN KỶ LUẬT")
        </th>
        <th>
            @Html.DisplayName("QUYẾT ĐỊNH KỶ LUẬT")
        </th>
        <th>
            @Html.DisplayName("MÃ NHÂN VIÊN BỊ KỶ LUẬT")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.ThoiGian)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QuyetDinh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaNV)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.MaKL }) |
                @Html.ActionLink("Details", "Details", new { id = item.MaKL }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.MaKL })

                

            </td>
        </tr>
    }

</table>


